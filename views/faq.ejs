<% include navbar.ejs %>
<div class="main" style="margin-top:15px;">
    <div class="container">
        <form role="form" action="/faq" method="post">
            <div class="form-group">
                <label for="questionsTitle">问题标题</label>
                <input type="text" name="title" class="form-control" id="questionsTitle" placeholder="概括问题">
            </div>
            <div class="form-group">
                <label for="questionsDescription">问题描述</label>
                <textarea rows="10" name="description" class="form-control" id="questionsDescription" placeholder="这里是问题的详细描述或者解决方案" style="max-height:450px;overflow-y: scroll;"></textarea>
            </div>
            <div class="form-group">
                <button type="submit" class="btn btn-success">提 交</button>
            </div>
        </form>
    </div>
</div>


